{
  "name": "CpuTab",
  "version": "0.0.1",
  "description": "Adds CPU usage to tab description",
  "permissions": [
    "tabs",
    "processes",
    "http://*/",
    "https://*/",
    "storage"
  ],
  "background": {
      "scripts": ["background.js"],
      "persistent": false
    },
  "browser_action": {
      "default_title": "CpuTab",
      "default_icon": {
        "16": "images/icon_16.png",
        "32": "images/icon_32.png",
        "48": "images/icon_48.png",
        "128": "images/icon_128.png"
      }
    },
    "icons": {
      "16": "images/icon_16.png",
      "32": "images/icon_32.png",
      "48": "images/icon_48.png",
      "128": "images/icon_128.png"
    },
  "manifest_version": 2
}
